STEP_01
lista = [6,8,3,1,3,2,5,7,9]
listaf = [6,8,9,7,6,5,4,5,6,3,2,1,3.4,3.6,8.9,9.9,2.1,5.3]
listas = ['a', 'A', 'b', 'B', 'z', 'Z']
listas.sort(key=str.lower, reverse=False)
print(listas)

STEP_02
lista = [6,8,3,1,3,2,5,7,9]
listaf = [6,8,9,7,6,5,4,5,6,3,2,1,3.4,3.6,8.9,9.9,2.1,5.3]
listas = ['a', 'A', 'b', 'B', 'z', 'Z']
lista_complex =[('juan', 20), ('paco', 40), ('maria', 23), ('julia', 50)]
lista_complex.sort(key=lambda x:x[1] ,reverse=True)
print(lista_complex)
